{
  "name": "ai-agent-network-backend",
  "version": "1.0.0",
  "description": "WebSocket relay server for the AI Agent Network enabling secure agent-to-agent communication",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t ai-agent-network-backend .",
    "docker:run": "docker run -p 3001:3001 ai-agent-network-backend"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "firebase-admin": "^11.10.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.2",
    "joi": "^17.10.1",
    "jose": "^4.14.6",
    "libsodium-wrappers": "^0.7.10",
    "on-finished": "^2.4.1",
    "rate-limiter-flexible": "^2.4.1",
    "socket.io": "^4.7.2",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/on-finished": "^2.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "author": "AI Agent Network Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-agent-network/ai-agent-network"
  },
  "keywords": [
    "websocket",
    "agent",
    "communication",
    "privacy",
    "socket.io",
    "redis",
    "firebase",
    "typescript",
    "express"
  ]
}