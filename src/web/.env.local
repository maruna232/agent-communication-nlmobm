NEXT_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=ai-agent-network.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=ai-agent-network
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=ai-agent-network.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789012
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890

# OpenAI API Configuration
NEXT_PUBLIC_OPENAI_API_KEY=sk-your-openai-api-key

# Google Calendar API Configuration
NEXT_PUBLIC_GOOGLE_API_KEY=your-google-api-key
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com
NEXT_PUBLIC_GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret

# WebSocket Server Configuration
NEXT_PUBLIC_WEBSOCKET_URL=wss://api.aiagentnetwork.com

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:8080/api

# Feature Flags
NEXT_PUBLIC_ENABLE_CALENDAR_INTEGRATION=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Development Settings
NEXT_PUBLIC_DEBUG_MODE=true
NEXT_PUBLIC_MOCK_SERVICES=false

# Security Settings
NEXT_PUBLIC_ENCRYPTION_SALT=random-encryption-salt-string-keep-this-secret